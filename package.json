{
	"name": "promptspree",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npx vite dev --host",
		"start:test": "cross-env NODE_ENV=test npm start",
		"backend": "node scripts/pocketbaseServe",
		"backend:test": "npm run backend -- 8091",
		"setup": "npm install && npm run setup:pocketbase && npx playwright install",
		"setup:pocketbase": "node scripts/pocketbaseSetup",
		"setup:pocketbase:test": "node scripts/pocketbaseSetupTest",
		"test": "npx playwright test",
		"test:trace": "npx playwright show-trace",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.31.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.1.4",
		"@sveltejs/kit": "^1.0.0",
		"@trivago/prettier-plugin-sort-imports": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"adm-zip": "^0.5.10",
		"axios": "^1.3.3",
		"cross-env": "^7.0.3",
		"date-fns": "^2.29.3",
		"dotenv": "^16.0.3",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"openai": "^3.1.0",
		"pocketbase": "^0.10.1",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.57.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
